{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "default": "Restaurant App",
      "required": true
    },
    "clients": {
      "type": "text"
    },
    "roles": {
      "type": "text"
    },
    "date_start": {
      "type": "date"
    },
    "date_end": {
      "type": "date"
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "categories": {
      "type": "text",
      "required": true,
      "default": "software"
    },
    "slug": {
      "type": "string"
    },
    "thumnail_img": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "main_img": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "ordering": {
      "type": "integer",
      "min": 1,
      "default": 100,
      "required": true,
      "unique": true
    }
  }
}
