{
  "kind": "collectionType",
  "collectionName": "portfolios",
  "info": {
    "name": "portfolio"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "default": "Restaurant App",
      "required": true
    },
    "category": {
      "type": "string",
      "default": "APP DESIGN",
      "required": true
    },
    "desc": {
      "type": "string",
      "required": true,
      "default": "Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod tempor"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": true
    }
  }
}
